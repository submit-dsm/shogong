{
  "name": "@service/server",
  "packageManager": "yarn@3.5.0",
  "main": "src/app.ts",
  "scripts": {
    "build": "tsc",
    "start": "NODE_ENV=production node dist/app.js",
    "dev": "nodemon --exec ts-node src/app.ts",
    "db:pull": "yarn pnpify prisma db pull",
    "db:push": "yarn pnpify prisma db push",
    "prisma:generate": "yarn pnpify prisma generate"
  },
  "dependencies": {
    "@prisma/client": "4.13.0",
    "@yarnpkg/pnpify": "^4.0.0-rc.42",
    "bcrypt": "^5.1.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@package/api-type": "workspace:^",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.16.0",
    "@types/uuid": "^9.0.1",
    "cors": "^2.8.5",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.22",
    "prisma": "4.13.0",
    "ts-node": "^10.9.1"
  }
}
